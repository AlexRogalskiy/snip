{
  "name": "sniptt",
  "version": "0.0.38",
  "description": "The secret manager that developers love!",
  "main": "./build/cli.js",
  "bin": {
    "sniptt": "./build/cli.js",
    "snip": "./build/cli.js"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/assets",
    "/build"
  ],
  "keywords": [
    "cli",
    "blockchain",
    "openpgp",
    "end-to-end-encryption",
    "secrets-management"
  ],
  "author": "sniptt.com <support+npm@sniptt.com> (https://github.com/sniptt-official)",
  "license": "MIT",
  "homepage": "https://github.com/sniptt-official/cli",
  "bugs": "https://github.com/sniptt-official/cli/issues",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "package:linux": "pkg . --no-bytecode --targets node14-linux-x64 --output bin/linux/snip",
    "package:macos": "pkg . --no-bytecode --targets node14-macos-x64 --output bin/macos/snip",
    "format": "prettier --loglevel warn --write \"**/*.{ts,js,json}\"",
    "format:check": "prettier --loglevel warn --check \"**/*.{ts,js,json}\""
  },
  "dependencies": {
    "chalk": "4.1.1",
    "cli-table": "0.3.6",
    "enquirer": "2.3.6",
    "fs-extra": "10.0.0",
    "got": "11.8.2",
    "keytar": "7.7.0",
    "openpgp": "5.0.0-2",
    "ora": "5.4.0",
    "unique-names-generator": "4.5.0",
    "yargs": "17.0.1",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@types/cli-table": "0.3.0",
    "@types/fs-extra": "9.0.11",
    "@types/node": "15.6.0",
    "@types/yargs": "17.0.0",
    "pkg": "5.2.1",
    "prettier": "2.3.0",
    "typescript": "4.2.4"
  },
  "pkg": {
    "scripts": "build/**/*.js"
  }
}
