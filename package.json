{
  "name": "sniptt",
  "description": "Secure content management with end-to-end encryption",
  "version": "0.0.19",
  "author": "sniptt.com <support+npm@sniptt.com> (https://github.com/sniptt-official)",
  "bin": {
    "sniptt": "./bin/run",
    "snip": "./bin/run"
  },
  "bugs": "https://github.com/sniptt-official/cli/issues",
  "dependencies": {
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/errors": "1.3.4",
    "@oclif/plugin-help": "3.2.2",
    "ascii-art-ansi": "1.3.7",
    "ascii-art-image": "1.3.6",
    "base64url": "3.0.1",
    "chalk": "4.1.1",
    "cli-ux": "5.5.1",
    "enquirer": "2.3.6",
    "fs-extra": "9.1.0",
    "got": "11.8.2",
    "openpgp": "5.0.0-1",
    "tslib": "2.2.0",
    "unique-names-generator": "4.5.0",
    "uuid": "8.3.2",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.26.0",
    "@oclif/test": "1.2.8",
    "@types/chai": "4.2.17",
    "@types/faker": "5.5.3",
    "@types/fs-extra": "9.0.11",
    "@types/mocha": "8.2.2",
    "@types/node": "15.0.1",
    "@types/uuid": "8.3.0",
    "chai": "4.3.4",
    "eslint": "5.16.0",
    "eslint-config-oclif": "3.1.0",
    "eslint-config-oclif-typescript": "0.2.0",
    "faker": "5.5.3",
    "globby": "10.0.2",
    "mocha": "8.3.2",
    "nock": "13.0.11",
    "nyc": "14.1.1",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src",
    "/sniptt-logo.png"
  ],
  "homepage": "https://github.com/sniptt-official/cli",
  "keywords": [
    "sniptt",
    "cli",
    "encryption",
    "end-to-end encryption",
    "content management",
    "pgp",
    "openpgp"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sniptt",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "sniptt-official/cli",
  "scripts": {
    "lint": "eslint \"**/*.ts\" --fix",
    "lint:check": "eslint \"**/*.ts\"",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha -r ts-node/register --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
